$(function(){exitIntent();});const exitIntent=function(){$('.js-exitIntent').each(function(){const $layout=$(this);$layout.on('click',function(e){e.preventDefault();let overlay=$('.exitIntent');overlay.find('.js-accept').attr("href",this.href);overlay.show().animate({"height":'100%'}).focus();overlay.find('.js-close').on('click',function(){overlay.animate({"height":'0'}).hide();});});});}